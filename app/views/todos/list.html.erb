<div class="row">
  <div class="col-md-6">
  <h3>Completed:</h3>
<% @todos.each do |elemento| %>
 <% if elemento[:completed] %>
    <h3><%= elemento[:description] %> </h3>
  <% end %>
 <%end  %> 
  </div>

  <div class="col-md-6">
    <h3>No Completed:</h3>
<% @todos.each do |elemento| %>
 <% if !elemento[:completed] %>
    <h3><%= elemento[:description] %></h3>
  <% end %>
<% end %>
  </div>
</div>